        "command": "pnpm install && pnpm run build",
        "publish": "dist",
        "environment": {
          "NODE_VERSION": "20.11.1",
          "PNPM_VERSION": "9.7.0"
        }
      }
    },
    "supabase": {
      "projectRef": "cuxzzpsyufcewtmicszk",
      "url": "https://cuxzzpsyufcewtmicszk.supabase.co",
      "database": {
        "tables": 16,
        "migrations": 17,
        "rlsEnabled": true
      },
      "auth": {
        "providers": ["email"],
        "jwtExpiry": 3600
      },
      "storage": {
        "buckets": ["course-materials", "user-uploads", "certificates", "generated-content"]
      }
    },
    "cloudflare": {
      "workers": {
        "name": "autopilot-deploy-worker",
        "routes": [
