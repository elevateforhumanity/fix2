{
  "version": "1.0.0",
  "integrations": {
    "netlify": {
      "siteId": "12f120ab-3f63-419b-bc49-430f043415c1",
      "siteName": "elevateforhumanityfix2",
      "url": "https://elevateforhumanityfix2.netlify.app",
      "functions": {
        "directory": "netlify/functions",
        "count": 17,
        "runtime": "nodejs20.x"
