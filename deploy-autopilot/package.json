{
  "name": "efh-deploy-autopilot",
  "version": "1.0.0",
  "type": "module",
  "description": "API-first deployment autopilot for EFH LMS",
  "scripts": {
    "start": "node --env-file=.env --import tsx src/index.ts --config tasks.yaml",
    "dry": "node --env-file=.env --import tsx src/index.ts --config tasks.yaml --dry",
    "deploy": "npm start"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "js-yaml": "^4.1.0",
    "ora": "^8.0.1",
    "tsx": "^4.19.1",
    "zod": "^3.23.8"
  }
}
