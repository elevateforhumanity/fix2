{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron/morning-reminders",
      "schedule": "0 8 * * 1-5"
    },
    {
      "path": "/api/cron/missed-checkins",
      "schedule": "0 10 * * 1-5"
    },
    {
      "path": "/api/cron/end-of-day-summary",
      "schedule": "0 17 * * 1-5"
    }
  ]
}
