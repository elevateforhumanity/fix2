        },
        {
          "table": "courses",
          "expected_count": 17
        },
        {
          "table": "modules",
          "expected_count": "50+"
        }
      ]
    },
    {
      "step": 5,
      "action": "verify-website",
      "url": "https://fix2-gpql-r0x49ne29-elevate-48e460c9.vercel.app/admin/courses",
      "expected": "no blue banner, 17 courses visible"
    }
  ],
  "success_criteria": {
    "programs_created": 16,
    "courses_created": 17,
    "modules_created": 50,
    "website_updated": true,
    "blue_banner_removed": true
