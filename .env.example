# Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to GitHub - it's in .gitignore

# ============================================
# GOOGLE ANALYTICS
# ============================================
# Get from: https://analytics.google.com/
# Format: G-XXXXXXXXXX
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
GOOGLE_TAG_MANAGER_ID=GTM-XXXXXXX

# ============================================
# GOOGLE SEARCH CONSOLE
# ============================================
# Get from: https://search.google.com/search-console
GOOGLE_SITE_VERIFICATION=YOUR_GOOGLE_VERIFICATION_CODE

# ============================================
# BING WEBMASTER TOOLS
# ============================================
# Get from: https://www.bing.com/webmasters
BING_SITE_VERIFICATION=YOUR_BING_VERIFICATION_CODE

# ============================================
# STRIPE PAYMENT PROCESSING
# ============================================
# Get from: https://dashboard.stripe.com/apikeys
# Use test keys for development, live keys for production
STRIPE_PUBLISHABLE_KEY=pk_live_XXXXXXXXXX
STRIPE_SECRET_KEY=sk_live_XXXXXXXXXX

# ============================================
# SOCIAL MEDIA AUTOMATION (3x Daily Posting)
# ============================================
# Facebook Page 1 (Main)
FACEBOOK_PAGE_1_ID=YOUR_PAGE_ID
FACEBOOK_PAGE_1_TOKEN=YOUR_PAGE_ACCESS_TOKEN
FACEBOOK_PAGE_URL=https://www.facebook.com/elevateforhumanity

# Facebook Page 2 (Programs)
FACEBOOK_PAGE_2_ID=YOUR_PAGE_ID
FACEBOOK_PAGE_2_TOKEN=YOUR_PAGE_ACCESS_TOKEN

# Facebook App (for API access)
FACEBOOK_APP_ID=YOUR_FACEBOOK_APP_ID
FACEBOOK_APP_SECRET=YOUR_FACEBOOK_APP_SECRET

# LinkedIn Company Page
LINKEDIN_COMPANY_URL=https://www.linkedin.com/company/elevateforhumanity
LINKEDIN_COMPANY_ID=YOUR_COMPANY_ID
LINKEDIN_ACCESS_TOKEN=YOUR_LINKEDIN_ACCESS_TOKEN
LINKEDIN_CLIENT_ID=YOUR_CLIENT_ID
LINKEDIN_CLIENT_SECRET=YOUR_CLIENT_SECRET

# YouTube Channel
YOUTUBE_CHANNEL_URL=https://www.youtube.com/@elevateforhumanity
YOUTUBE_CHANNEL_ID=YOUR_CHANNEL_ID
YOUTUBE_API_KEY=YOUR_YOUTUBE_API_KEY

# Twitter/X
TWITTER_HANDLE=@elevateforhumanity
TWITTER_API_KEY=YOUR_TWITTER_API_KEY
TWITTER_API_SECRET=YOUR_TWITTER_API_SECRET
TWITTER_ACCESS_TOKEN=YOUR_ACCESS_TOKEN
TWITTER_ACCESS_SECRET=YOUR_ACCESS_SECRET

# Instagram
INSTAGRAM_ACCESS_TOKEN=YOUR_INSTAGRAM_ACCESS_TOKEN
INSTAGRAM_BUSINESS_ACCOUNT_ID=YOUR_BUSINESS_ACCOUNT_ID

# Durable.co Blog Integration
DURABLE_API_KEY=YOUR_DURABLE_API_KEY
DURABLE_BLOG_URL=https://elevateforhumanity.durable.co/blog

# Zapier Webhook (for automation workflows)
ZAPIER_WEBHOOK_URL=https://hooks.zapier.com/hooks/catch/YOUR_WEBHOOK_ID

