# Required permissions: Workers Scripts:Edit, KV:Edit, R2:Edit, Account:Read
CLOUDFLARE_API_TOKEN=cf_tkn_XXXXXXXXXXXXXXXXXXXXXXXX
CLOUDFLARE_ACCOUNT_ID=acc_XXXXXXXXXXXXXXXX
CF_API_TOKEN=cf_tkn_XXXXXXXXXXXXXXXXXXXXXXXX
CF_ACCOUNT_ID=acc_XXXXXXXXXXXXXXXX

