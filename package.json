{
  "name": "elevateforhumanity",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build --outDir dist",
    "preview": "vite preview --host 0.0.0.0 --port 5173",
    "start": "node ./server.js || serve -s dist -l 3000",
    "postbuild": "node scripts/fix-domain-urls.js && node scripts/update-canonical-urls.js || true",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx || true",
    "test": "echo \"No tests configured\" && exit 0",
    "env:check": "node scripts/verify-production.mjs || true"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "vite": "^4.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^8.0.0"
  }
}